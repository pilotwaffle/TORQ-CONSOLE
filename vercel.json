{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "torq-console",
  "builds": [
    {
      "src": "torq_console/ui/railway_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "torq_console/ui/railway_app.py"
    }
  ],
  "env": {
    "TORQ_CONSOLE_PRODUCTION": "true",
    "TORQ_DISABLE_LOCAL_LLM": "true",
    "TORQ_DISABLE_GPU": "true",
    "OPENAI_API_KEY": "@openai_api_key",
    "PYTHON_VERSION": "3.11"
  }
}
